{
  "name": "@wspro/ets-client",
  "version": "1.0.0",
  "description": "ETS Client",
  "author": "https://github.com/wsp-repo/",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsp-repo/ets-client.git"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "build:nest": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{ts,js}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.5.5",
    "@nestjs/microservices": "^7.5.5",
    "@nestjs/platform-express": "^7.5.5",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.5",
    "@types/express": "^4.17.9",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.14.10",
    "@types/request": "^2.48.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@wspro/eslint": "1.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "jest": "^26.5.3",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --max-warnings 0",
      "eslint --quiet --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
    "verbose": true,
    "bail": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": "\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "prettier": "@wspro/eslint"
}
